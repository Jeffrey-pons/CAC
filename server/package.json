    "@nestjs/testing": "^10.3.4",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "mongoose": "^8.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
